# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'weiBo' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for weiBo
    pod 'MJRefresh'
    pod 'SVProgressHUD', :inhibit_warnings => true
    
    # Swift
    pod 'Alamofire'
    pod 'Kingfisher'
    pod 'HandyJSON', '~> 4.0.0-beta.1', :inhibit_warnings => true
    pod 'SnapKit'
    pod 'XCGLogger', :inhibit_warnings => true
    pod 'ImageViewer'
    pod 'GTMWebKit'
end
